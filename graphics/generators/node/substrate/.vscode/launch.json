{
  "version": "0.2.0",
  "configurations": [



      {
          "type": "node",
          "request": "attach",
          "name": "Node: Nodemon",
          "processId": "${command:PickProcess}",
          "port": 9229,
          "restart": true,
          "smartStep": true,
          "stopOnEntry": true,
                    "protocol": "inspector",
          "sourceMaps": true,
          "outFiles": ["${workspaceFolder}/dist/**/*.js"],
          "skipFiles": [
            "${workspaceFolder}/node_modules/**/*.js",
            "${workspaceFolder}/node_modules/**/*.ts",
            "<node_internals>/**/*.js",
            "*VM*"
          ]

      },
  ]
}
